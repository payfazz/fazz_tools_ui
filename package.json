{
  "name": "fazz-tools-ui",
  "description": "Flutter Debugging Tools",
  "version": "1.0.0",
  "author": {
    "name": "Team Payfazz",
    "version": "1.0.0",
    "email": "hantze.sudarma@cashfazz.com"
  },
  "dependencies": {
    "antd": "^3.18.1",
    "dotenv": "^8.0.0",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.0",
    "moment": "^2.29.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-json-view": "^1.19.1",
    "react-redux": "^7.0.3",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-observable": "^1.1.0",
    "rxjs": "^6.5.2",
    "websocket": "^1.0.28"
  },
  "homepage": ".",
  "main": "server/app/app.js",
  "scripts": {
    "start": "concurrently \"BROWSER=none PORT=7072 react-scripts start\" \"wait-on http://localhost:7072 && electron .\"",
    "preelectron-pack": "concurrently \"rimraf ./build ./dist\" \"react-scripts build\"",
    "electron-pack": "build -c.extraMetadata.main=./server/app/app.js"
  },
  "build": {
    "files": [
      "build/**/*",
      "server/**/*",
      "node_modules/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^5.0.1",
    "electron-builder": "^20.41.0",
    "electron-builder-squirrel-windows": "^20.42.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.1",
    "wait-on": "^3.2.0"
  }
}
